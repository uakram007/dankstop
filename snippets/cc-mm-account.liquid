<script>
  window.onload = () => {
    typeof CustomerAddresses !== 'undefined' && new CustomerAddresses();
  };
  // Get all tab buttons
  const tabItems = document.querySelectorAll('.cc-mm-account-nav-item');
  const tabContents = document.querySelectorAll('.cc-mm-account-tab-content');

  // Loop through each tab and add click event listener
  tabItems.forEach(tab => {
    tab.addEventListener('click', () => {

      // Remove active class from all tabs and contents
      tabItems.forEach(item => {
        item.classList.remove('active')
      });

      tabContents.forEach(content => content.style.display = 'none');
      // Add active class to the clicked tab and display the associated content
      const tabTarget = tab.getAttribute('data-tab');
      document.getElementById(tabTarget).style.display = 'block';
      tab.classList.add('active');

      if (window.screen.width < 801) {
        document.querySelector(`#${tabTarget}-mobile`).style.display = 'block';
      }
    });
  });

  // Initially display the first tab content
  document.getElementById('orders').style.display = 'block';
  if (window.screen.width < 801) {
    document.getElementById('orders-mobile').style.display = 'block';
  }
</script>
