          {%- continue -%}
        {%- endif -%}

        {%- comment -%}
        <a href="{{ product_collection.url }}" target="_blank" rel="noopener" class="related-collection grid-item col-6 col-md-4 col-lg-2">
          {%- include 'image-module', im_src: product_collection.image, im_id: img_id, im_dt_ws: img_widths -%}
          {{- product_collection.title -}}
        </a>
        {%- endcomment -%}
