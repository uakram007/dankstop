{% include 'breadcrumb' %}

<div class="wholesale-content">

  <div class="ws-header section">
    <div class="grid">
      <div class="grid-item">
        <span class="svg-span">{% include 'theme-icons', this_icon: 'wholesale-logo' %}</span>
      </div>
      <div class="grid-item">
        <h1>{{ 'wholesale.title' | t }}</h1>
      </div>
    </div>
  </div>

  <div class="container">
  
    <div class="ws-greeting section">
      <div class="grid">
        <div class="grid-item ws-greeting-text">
          <h2>
            <div>{{ 'wholesale.greeting_1' | t }}</div>
            <div>{{ 'wholesale.greeting_2' | t }}</div>
          </h2>
        </div>
        <div class="grid-item medium--one-half large--one-half">
          <p>{{ 'wholesale.paragraph_1' | t }}</p>
        </div>
        <div class="grid-item medium--one-half large--one-half">
          <p>{{ 'wholesale.paragraph_2' | t }}</p>
        </div>
        <div class="grid-item ws-account-check">
          {% assign wholesale_customer = false %}
          {% if customer %}

            {% for tag in customer.tags %}
              {% assign the_tag = tag | strip | downcase %}
              {% if the_tag == settings.wholesale_approval_tag %}
                {% assign wholesale_customer = true %}
              {% endif %}
            {% endfor %}

            {% if wholesale_customer %}

              {% if settings.wholesale_require_registration_approval %}

                {% assign customer_registration_tag = 'wholesale-applicant' %}

                {% for tag in customer.tags %}
                  {% assign the_tag = tag | strip | downcase %}
                  {% if the_tag == settings.wholesale_approval_tag %}
                    {% assign account_approved = true %}
                  {% endif %}
                {% endfor %}

                {% if account_approved %}
                  <p>{{ 'wholesale.wholesale_customer_approved_1' | t }}</p>
                  <p>{{ 'wholesale.wholesale_customer_approved_2' | t }}</p>
                {% else %}
                  <p>{{ 'wholesale.registered_not_approved' | t }}</p>
                {% endif %}

              {% else %}

                {% assign customer_registration_tag = 'wholesalizzle-clientizzle,wholesale-applicant' %}

                <p>{{ 'wholesale.wholesale_customer_approved_1' | t }}</p>
                <p>{{ 'wholesale.wholesale_customer_approved_2' | t }}</p>
              {% endif %}

            {% else %}
              <p>{{ 'wholesale.retail_customer' | t }}</p>
              <a href="#wholesale-form-container" class="btn pageScrollBtn">{{ 'wholesale.retail_customer_btn' | t }}</a>
            {% endif %}
          {% else %}
            <p>{{ 'wholesale.not_registered' | t }}</p>
            <a href="#wholesale-form-container" class="btn pageScrollBtn">{{ 'wholesale.not_registered_btn' | t }}</a>
          {% endif %}
        </div>
      </div>
    </div>

    <div class="ws-categories section">
      <div class="grid">
        <div class="grid-item ws-cat-header">
          <h2>{{ 'wholesale.categories_title' | t }}</h2>
          <span>{{ 'wholesale.categories_subtitle' | t }}</span>
        </div>
        <div class="grid-item ws-category">
