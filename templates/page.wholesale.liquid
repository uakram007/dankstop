                    <option value="Customer referral">{{ 'wholesale.reg_form.source_1' | t }}</option>
                    <option value="Social network">{{ 'wholesale.reg_form.source_2' | t }}</option>
                    <option value="Online ad">{{ 'wholesale.reg_form.source_3' | t }}</option>
                    <option value="Blog post">{{ 'wholesale.reg_form.source_4' | t }}</option>
                    <option value="Magazine ad">{{ 'wholesale.reg_form.source_5' | t }}</option>
                    <option value="Other">{{ 'wholesale.reg_form.source_6' | t }}</option>
