              </div>
              <div class="form-row notes-row">
                <div class="form-left"><label for="contact_note"><i class="fa fa-comment"></i><span>{{ 'wholesale.reg_form.notes' | t }}</span></label></div>
                <div class="form-right"><textarea name="customer[note][Notes:]" id="contact_note" rows="4" cols="50" placeholder="{{ 'wholesale.reg_form.notes_placeholder' | t }}"></textarea></div>
              </div>
              </div><!-- end #all-form-fields -->
              <div class="form-row response">
