            <input type="hidden" name="utf8" value="âœ“">

            {% if customer %}
            <input type="hidden" value="customer" name="form_type">
            <input id="update-me" type="hidden" name="contact[tags]" value="{{ customer_registration_tag }}" />    
            {% else %}
            <input type="hidden" value="create_customer" name="form_type">
            <input id="update-me" type="hidden" name="customer[tags]" value="{{ customer_registration_tag }}" />
            {% endif %}

            <fieldset>
              <legend>{{ 'wholesale.reg_form.title' | t }}</legend>
              <div id="all-form-fields">
              <div class="form-row subject-row disabled-field">
                <div class="form-left"><label for="form-subject"><i class="fa fa-pencil-square-o"></i><span>{{ 'wholesale.reg_form.subject' | t }}</span></label></div>
                <div class="form-right"><input type="text" name="form-subject" id="form-subject" placeholder="{{ 'wholesale.reg_form.subject_placeholder' | t }}" value="{{ 'wholesale.reg_form.subject_placeholder' | t }}" disabled="disabled" /></div>
              </div>
              <div class="form-row">
                <div class="form-left required-field"><label for="contact-first-name"><i class="fa fa-user"></i><span>{{ 'wholesale.reg_form.contact' | t }}</span></label></div>
                <div class="form-right">
                  <div class="input-line">
                    <div class="line-left first-name-line"><input type="text" name="customer[first_name]" id="contact-first-name" class="address-input" placeholder="{{ 'wholesale.reg_form.contact_first_placeholder' | t }}" /></div>
                    <div class="line-right last-name-line"><input type="text" name="customer[last_name]" id="contact-last-name" class="address-input" placeholder="{{ 'wholesale.reg_form.contact_last_placeholder' | t }}" /></div>
                  </div>
                </div>
              </div>
              <div class="form-row">
                <div class="form-left required-field"><label for="contact_phone"><i class="fa fa-phone"></i><span>{{ 'wholesale.reg_form.phone' | t }}</span></label></div>
                <div class="form-right"><input type="tel" name="customer[note][Phone #:]" id="contact_phone" placeholder="{{ 'wholesale.reg_form.phone_placeholder' | t }}" pattern=".{10,}" required="" /></div>
              </div>
              <div class="form-row">
                <div class="form-left required-field"><label for="contact_email"><i class="fa fa-envelope"></i><span>{{ 'wholesale.reg_form.email' | t }}</span></label></div>
                <div class="form-right">
                  {% if customer %}
                  <input type="email" name="customer[email]" id="contact_email" placeholder="{{ customer.email }}" value="{{ customer.email }}" required="" autocorrect="off" autocapitalize="off" />
                  {% else %}
                  <input type="email" name="customer[email]" id="contact_email" placeholder="{{ 'wholesale.reg_form.email_placeholder' | t }}" required="" autocorrect="off" autocapitalize="off" />
                  {% endif %}
                </div>
              </div>
              <div class="form-row password-row">
                <div class="form-left"><label for="account-password"><i class="fa fa-key"></i><span>{{ 'wholesale.reg_form.account_password' | t }}</span></label></div>
                <div class="form-right"><input type="password" name="customer[password]" id="account-password" placeholder="{{ 'wholesale.reg_form.account_password_placeholder' | t }}" /></div>
              </div>
              <div class="form-row">
                <div class="form-left"><label for="business_name"><i class="fa fa-building-o"></i><span>{{ 'wholesale.reg_form.business_name' | t }}</span></label></div>
                <div class="form-right"><input type="text" name="customer[note][Business Name:]" id="business_name" placeholder="{{ 'wholesale.reg_form.business_name_placeholder' | t }}" /></div>
              </div>
              <div class="form-row tax-id-row">
