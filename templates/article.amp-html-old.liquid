    <a href="/" class="mn-logo-cont">
      {%- if settings.logo_use_image -%}
      {%- assign logo_img_src = 'logo.png' | asset_url -%}
      {%- assign logo_img_alt = shop.name -%}
      {%- assign logo_img_title = 'general.dankstop.long_title' | t -%}
      {%- assign logo_img_size = 'small' -%}
      <span>Shop on</span>
      <span class="svg-cont">
        <svg id="dsLeafLogoPrimary" class="dankstop-logo-svg" fill="#39b54a" width="100%" height="100%" viewBox="0 0 197 243" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.1"><g class="dankstop-logo-svg--g"><path d="M99.6,12.3l-4-3.8l-4,3.8c-1.7,1.6-22,21-42.5,46.2l0.3,0.3c-5.5,8.8-6,17.5-4.8,24.6c0.4,2.5,1,4.7,1.8,6.8 c2.2,6.2,5.8,11.9,10.9,17l40,40c-4.3-0.1-7.4,0.4-8.2,0.6c-21.5,3.2-37.6,22.1-37.4,43.9c0.1,8.1,2.3,15.8,6.4,22.4 c2.1,3.4,4.6,6.6,7.6,9.4C72.7,230,81.4,234,90.8,235c1.5,0.2,3.1,0.3,4.7,0.3c0.1,0,0.1,0,0.2,0c0,0,0,0,0,0 c50.1,0,90.9-40.8,90.9-90.9C186.5,95.4,103.2,15.6,99.6,12.3z M127.9,193.7c-1.2,15.9-14,28.6-30,29.7c-0.3,0-0.6,0-0.9,0 c-0.8,0-1.6,0.1-2.4,0c-0.1,0-0.3,0-0.4,0c-1.4,0-2.9-0.1-4.3-0.2c-0.5,0.1-1,0.1-1.5,0.2c-6.3,0.4-12.3-1-17.7-4 c6.1,2,12.5,3.3,19.2,3.8c0.3,0,0.7-0.1,1-0.1c-9.3-1.3-17.6-6.6-23-15.2c-0.8-1.3-1.6-2.7-2.1-4.2c-8.3-20.7,5.3-41.7,25.1-44.6 l0.3-0.1c0.1,0,0.3,0,0.6-0.1c0.8-0.1,2.5-0.3,4.7-0.3c5.1,0,13.1,1.1,19.6,7.1l1.5,1.5l0,0C124.8,174.1,128.6,183.7,127.9,193.7z M133.2,214.1c3.5-5.8,5.8-12.4,6.3-19.5c1-13.4-4-26.2-13.6-35.3c-0.6-0.7-1.2-1.3-1.9-1.9L65.5,98.9c-3.8-3.8-6.6-8.1-8.1-12.6 c-3.2-9.1-1.7-17.5,4.5-25l0,0c13.9-16.6,27.3-30.3,33.7-36.7c7.1,7.1,22.7,23,38.1,41.9c26.9,33,41.1,59.9,41.1,77.8 C174.8,174.5,158,200.7,133.2,214.1z"></path><path d="M44.4,204.7c-17.1-14.5-28-36.2-28-60.3c0-12.9,7.4-30.4,21.7-51.6c-0.4-0.9-0.7-1.7-1-2.6 c-1.3-3.7-2.3-8.2-2.3-13.1c-16.4,22.8-30,47.5-30,67.3c0,35,21.2,66.6,50.3,81.7c0,0-4.5-5.2-7.6-11.6 C45.4,210.3,44.4,204.7,44.4,204.7z"></path></g></svg>
