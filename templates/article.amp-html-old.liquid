    <div class="ds-benefits">
        <div class="dsb-1">Free Same-Day Shipping</div>
        <div class="dsb-2">Discreet Packaging</div>
        <div class="dsb-3">Hassle-Free Returns</div>
    </div>

    <div class="grid grid-border">
      <div class="grid-item large--four-fifths">
        
        <header class="section-header">
          <div class="section-header--left">
            <h1 class="text-left">{{ article.title }}</h1>
            <p class="text-light">
              {%- assign date_published_ISO = article.published_at | date: '%Y-%m-%d' -%}
              {%- assign date_published_standard = article.published_at | date: format: 'month_day_year' -%}
              {%- capture date -%}<time datetime="{{ date_published_ISO }}">{{ date_published_standard }}</time>{%- endcapture -%}
              {%- capture author -%}{{ article.author }}{%- endcapture -%}
              {%- comment -%}
              {{ 'blogs.article.author_on_date_html' | t: author: author, date: date }}
              {%- endcomment -%}
              <span class="article--author">{{ author }}&nbsp; &#8226;</span>
              <span class="article--published-date"><span class="dot-sep"></span>{{ date }}<!-- &nbsp; &#8226; --></span>
              {%- comment -%}
              <span class="article--time-to-read"><span class="dot-sep"></span>&nbsp;{{ minutes_to_read }}&nbsp;{{ 'blogs.article.min_to_read' | t }}</span>
              {%- endcomment -%}
            </p>
          </div>
        </header>
        
        <div class="rte">
          {%- comment -%}
          <!-- Remove CSS styles attributes -->
          {%- endcomment -%}
          {%- assign article_content_raw = article.content -%}
          {%- assign style_attr_array = article_content_raw | split: 'style="' -%}
          {%- assign style_attr_count = style_attr_array.size -%}
          {%- for i in (1..style_attr_count) -%}
            {%- assign value_of_style_attr = '' -%}
            {%- assign style_attr_with_value = '' -%}
            {%- assign replace_with_value = '' -%}
            {%- if i == 1 -%}
              {%- assign content_split_at_style = article_content_raw | split: ' style="' -%}
              {%- assign content_post_first_style = content_split_at_style[1] -%}
              {%- assign value_of_style_attr = content_post_first_style | split: '"' | first -%}
              {%- assign style_attr_with_value = ' style="' | append: value_of_style_attr | append: '"' -%}
              {%- assign article_content_AMP = article_content_raw | replace: style_attr_with_value, '' -%}
            {%- else -%}
              {%- assign content_split_at_style = article_content_AMP | split: ' style="' -%}
              {%- assign content_post_first_style = content_split_at_style[1] -%}
              {%- assign value_of_style_attr = content_post_first_style | split: '"' | first -%}
              {%- assign style_attr_with_value = ' style="' | append: value_of_style_attr | append: '"' -%}
              {%- if value_of_style_attr contains 'float: right' or value_of_style_attr contains 'float:right' -%}
                {%- assign replace_with_value = ' class="img-float img-fr"' -%}
              {%- elsif value_of_style_attr contains 'float: left' or value_of_style_attr contains 'float:left' -%}
                {%- assign replace_with_value = ' class="img-float img-fl"' -%}
              {%- endif -%}
              {%- assign article_content_AMP = article_content_AMP | replace: style_attr_with_value, replace_with_value -%}
            {%- endif -%}
            {%- assign styles_leftover_array = article_content_AMP | split: 'style="' -%}
            {%- assign styles_leftover_count = styles_leftover_array.size -%}
            {%- if styles_leftover_count < 2 -%}
              {%- break -%}
            {%- else -%}
            {%- endif -%}
          {%- endfor -%}
          
          
          {%- comment -%}
          <!-- Remove CSS style tags -->
          {%- endcomment -%}
          {%- assign tag_to_remove_start = '<style>' -%}
          {%- assign tag_to_remove_end = '</style>' -%}
          {%- assign tag_attr_array = article_content_AMP | split: tag_to_remove_start -%}
          {%- assign tag_attr_count = tag_attr_array.size | minus: 1 -%}
          {%- for i in (1..tag_attr_count) -%}
            {%- assign content_split_at_tag = article_content_AMP | split: tag_to_remove_start | join: '' -%}
            {%- assign article_content_AMP = content_split_at_tag | split: tag_to_remove_end | last -%}
          {%- endfor -%}
          
          
          {%- comment -%}
          <!-- Process iFrame tags -->
          {%- endcomment -%}
          {%- assign iframe_closing_tag_replacement = '</amp-iframe>' -%}
          {%- assign tag_to_process_start = '<iframe ' -%}
          {%- assign tag_to_process_end = '</iframe>' -%}
          {%- assign tag_attr_array = article_content_AMP | split: tag_to_process_start -%}
          {%- assign tag_attr_count = tag_attr_array.size | minus: 1 -%}
          {%- for i in (1..tag_attr_count) -%}
            {%- assign content_split_at_tag = article_content_AMP | split: tag_to_process_start -%}
            {%- assign content_of_tag = content_split_at_tag[1] | split: tag_to_process_end | first -%}
            {%- assign original_tag = tag_to_process_start | append: content_of_tag | append: tag_to_process_end -%}
          
            {%- if content_of_tag contains 'youtube.com' -%}
              {%- assign iframe_start_tag_replacement = '<amp-iframe layout="responsive" sandbox="allow-scripts allow-same-origin allow-popups" frameborder="0"' -%}
            {%- else -%}
              {%- assign iframe_start_tag_replacement = '<amp-iframe style="top:75vh;" layout="responsive" sandbox="allow-scripts allow-same-origin allow-popups" frameborder="0"' -%}
            {%- endif -%}
            {%- assign replacement_tag = iframe_start_tag_replacement | append: content_of_tag | append: iframe_closing_tag_replacement -%}
          
            {%- assign article_content_AMP = article_content_AMP | replace: original_tag, replacement_tag -%}
          {%- endfor -%}

          
          {%- comment -%}
          <!-- Remove prohibited percentages off width/height attributes -->
          {%- endcomment -%}
          {%- assign percentage_attr_value_selector = '%"' -%}
          {%- assign percentage_attr_value_replc = '"' -%}
          {%- assign article_content_AMP = article_content_AMP | replace: percentage_attr_value_selector, percentage_attr_value_replc -%}
          
          
          
          {%- comment -%}
          <!-- Remove prohibited percentages off width/height attributes -->
          {%- endcomment -%}
          {%- assign percentage_attr_value_selector = '%"' -%}
          {%- assign percentage_attr_value_replc = '"' -%}
          {%- assign article_content_AMP = article_content_AMP | replace: percentage_attr_value_selector, percentage_attr_value_replc -%}
          
          
          
          {%- comment -%}
          <!-- Remove prohibited percentages off width/height attributes -->
          {%- endcomment -%}
          {%- assign empty_target = 'target=""' -%}
          {%- assign target_blank = 'target="_blank" rel="noopener"' -%}
          {%- assign article_content_AMP = article_content_AMP | replace: empty_target, target_blank -%}
          
          
          
          {%- comment -%}
          <!-- Remove Disallowed Properties -->
          {%- endcomment -%}
          {%- assign strings_to_remove_string = 'mce-data-marked="1", bis_skin_checked="1"' -%}
          {%- assign strings_to_remove_array = strings_to_remove_string | split: ', ' -%}
          {%- for string_to_remove in strings_to_remove_array -%}
          {%- assign article_content_AMP = article_content_AMP | replace: string_to_remove, '' -%}
          {%- endfor -%}
          
          
          
          {%- comment -%}
          <!-- Append Disallowed Properties that are difficult to target -->
          {%- endcomment -%}
          {%- assign strings_to_replace_string = 'natural-width=, natural-height=' -%}
          {%- assign strings_to_replace_array = strings_to_replace_string | split: ', ' -%}
          {%- for string_to_replace in strings_to_replace_array -%}
          {%- comment -%}
          {%- assign string_to_replace_with = "data-" | append: string_to_replace -%}
          {%- endcomment -%}
          {%- assign string_to_replace_with = string_to_replace | replace: "natural-", ""-%}
          {%- assign article_content_AMP = article_content_AMP | replace: string_to_replace, string_to_replace_with -%}
          {%- endfor -%}
          
          
          {%- comment -%}
          <!-- Append Disallowed Properties that are difficult to target -->
          {%- endcomment -%}
          {%- assign strings_to_append_string = 'color="' -%}
          {%- assign strings_to_append_array = strings_to_append_string | split: ', ' -%}
          {%- for string_to_append in strings_to_append_array -%}
          {%- assign string_to_replace_with = "data-" | append: string_to_append -%}
          {%- assign article_content_AMP = article_content_AMP | replace: string_to_append, string_to_replace_with -%}
          {%- endfor -%}
          
          

          {%- comment -%}
          <!-- Remove JS script tags -->
          {%- endcomment -%}
          {%- assign elements_to_remove_string = 'script,font' -%}
          {%- assign elems_array = elements_to_remove_string | split: ',' -%}
          {%- assign article_content_raw = article.content -%}
          {%- for element in elems_array -%}
            {%- assign tag_to_remove_start = '<' | append: element | append: ' ' -%}
            {%- assign tag_to_remove_end = '</' | append: element | append: '>' -%}
            {%- assign tag_attr_array = article_content_raw | split: tag_to_remove_start -%}
            {%- assign tag_attr_count = tag_attr_array.size -%}
            {%- for i in (1..tag_attr_count) -%}
              {%- assign value_of_tag_attr = '' -%}
              {%- assign tag_attr_with_value = '' -%}
              {%- assign replace_with_value = '' -%}
              {%- if i == 1 -%}
                {%- assign content_split_at_tag = article_content_AMP | split: tag_to_remove_start -%}
                {%- assign content_post_first_tag = content_split_at_tag[1] -%}
                {%- assign value_of_tag_attr = content_post_first_tag | split: tag_to_remove_end | first -%}
                {%- assign tag_attr_with_value = tag_to_remove_start | append: value_of_tag_attr | append: tag_to_remove_end -%}
                {%- assign article_content_AMP = article_content_AMP | replace: tag_attr_with_value, '' -%}
              {%- else -%}
                {%- assign content_split_at_tag = article_content_AMP | split: tag_to_remove_start -%}
                {%- assign content_post_first_tag = content_split_at_tag[1] -%}
                {%- assign value_of_tag_attr = content_post_first_tag | split: tag_to_remove_end | first -%}
                {%- assign tag_attr_with_value = tag_to_remove_start | append: value_of_tag_attr | append: tag_to_remove_end -%}
                {%- assign article_content_AMP = article_content_AMP | replace: tag_attr_with_value, replace_with_value -%}
              {%- endif -%}
              {%- assign tag_leftover_array = article_content_AMP | split: tag_to_remove_start -%}
              {%- assign tag_leftover_count = tag_leftover_array.size -%}
              {%- if tag_leftover_count < 2 -%}
                {%- break -%}
              {%- else -%}
              {%- endif -%}
            {%- endfor -%}
          {%- endfor -%}
          
          {%- comment -%}
          <!-- Remove JS script tags -->
          {%- endcomment -%}
          {%- assign tag_to_remove_start = '<script>' -%}
          {%- assign tag_to_remove_end = '</script>' -%}
          {%- assign article_content_raw = article.content -%}
          {%- assign tag_attr_array = article_content_raw | split: tag_to_remove_start -%}
          {%- assign tag_attr_count = tag_attr_array.size -%}
          {%- for i in (1..tag_attr_count) -%}
            {%- assign value_of_tag_attr = '' -%}
            {%- assign tag_attr_with_value = '' -%}
            {%- assign replace_with_value = '' -%}
            {%- if i == 1 -%}
              {%- assign content_split_at_tag = article_content_AMP | split: tag_to_remove_start -%}
              {%- assign content_post_first_tag = content_split_at_tag[1] -%}
              {%- assign value_of_tag_attr = content_post_first_tag | split: tag_to_remove_end | first -%}
              {%- assign tag_attr_with_value = tag_to_remove_start | append: value_of_tag_attr | append: tag_to_remove_end -%}
              {%- assign article_content_AMP = article_content_AMP | replace: tag_attr_with_value, '' -%}
            {%- else -%}
              {%- assign content_split_at_tag = article_content_AMP | split: tag_to_remove_start -%}
              {%- assign content_post_first_tag = content_split_at_tag[1] -%}
              {%- assign value_of_tag_attr = content_post_first_tag | split: tag_to_remove_end | first -%}
              {%- assign tag_attr_with_value = tag_to_remove_start | append: value_of_tag_attr | append: tag_to_remove_end -%}
              {%- assign article_content_AMP = article_content_AMP | replace: tag_attr_with_value, replace_with_value -%}
            {%- endif -%}
            {%- assign tag_leftover_array = article_content_AMP | split: tag_to_remove_start -%}
            {%- assign tag_leftover_count = tag_leftover_array.size -%}
            {%- if tag_leftover_count < 2 -%}
              {%- break -%}
            {%- else -%}
            {%- endif -%}
          {%- endfor -%}

          
          
          {%- comment -%}
          <!-- Process Imgs -- make into AMP tags + IMGIX compressed source -->
          {%- endcomment -%}
          {%- assign responsive_amp_imgs = true -%}
          {%- if responsive_amp_imgs -%}
          {%- assign imt_tag_AMP_start = '<amp-img layout="responsive" ' -%}
          {%- assign imt_tag_AMP_width = 1.6 -%}
          {%- assign imt_tag_AMP_height = 1 -%}
          {%- else -%}
          {%- assign imt_tag_AMP_start = '<amp-img layout="intrinsic" ' -%}
          {%- assign imt_tag_AMP_width = 320 -%}
          {%- assign imt_tag_AMP_height = 200 -%}
          {%- endif -%}
          {%- assign imt_tag_AMP_start_with_dimensions = imt_tag_AMP_start | append: 'width="' | append: imt_tag_AMP_width | append: '" height="' | append: imt_tag_AMP_height | append: '" ' -%}
          {%- assign imt_tag_AMP_end = '></amp-img>' -%}
          
          {%- assign imt_tag_array = article_content_AMP | split: '<img ' -%}
          {%- assign imt_tag_count = imt_tag_array.size -%}
          {%- for i in (1..imt_tag_count) -%}
            {%- assign img_tag_full = '' -%}
            {%- assign style_attr_with_value = '' -%}
            {%- assign content_split_at_img = article_content_AMP | split: '<img ' -%}
            {%- assign img_2nd_part = content_split_at_img[1] -%}
            {%- assign img_content = img_2nd_part | split: '>' | first -%}
            {%- assign img_tag_HTML = '<img ' | append: img_content | append: '>' -%}
          
            {%- comment -%}
            <!-- Replacing SRC with IMGIX Src -->
            {%- endcomment -%}
            {%- if enableImgix -%}
            {%- assign shopify_cdn_url = 'cdn.shopify.com' -%}
            {%- assign imgix_cdn_url = 'dankstop.imgix.net' -%}
            {%- assign imgix_params = 'auto=compress%2Cformat&q=50&w=' | append: imt_tag_AMP_width -%}
            {%- assign imgix_params_concatenator = '&' -%}
            {%- assign img_content_src = img_content | split: 'src="' | last | strip -%}
            {%- assign img_content_src = img_content_src | split: '"' | first -%}
            {%- assign img_content_src = img_content_src | split: '?' | first -%}
            {%- unless img_content_src contains '?' -%}
              {%- assign imgix_params_concatenator = '?' -%}
            {%- endunless -%}
            {%- assign img_content_IMGIX_src = img_content_src | replace: shopify_cdn_url, imgix_cdn_url | append: imgix_params_concatenator | append: imgix_params -%}
            {%- assign img_content = img_content | replace: img_content_src, img_content_IMGIX_src -%}
            {%- endif -%}
            
            {%- comment -%}
            <!-- Creating Final AMP-Img tag -->
            {%- endcomment -%}
            {%- if img_content contains 'width="' -%}
            {%- assign img_tag_AMP = imt_tag_AMP_start | append: img_content | append: imt_tag_AMP_end -%}
            {%- else -%}
            {%- assign img_tag_AMP = imt_tag_AMP_start_with_dimensions | append: img_content | append: imt_tag_AMP_end -%}
            {%- endif -%}          
            {%- assign article_content_AMP = article_content_AMP | replace: img_tag_HTML, img_tag_AMP -%}
          {%- endfor -%}

          
          {%- comment -%}
          <!-- Removing stray <img> tags -->
          {%- endcomment -%}
          {%- assign article_content_AMP = article_content_AMP | replace: '<img>', '' -%}
          
          
          {%- comment -%}
          <!-- Removing excessive meta tags -->
          {%- endcomment -%}
          {%- assign article_content_AMP_final = article_content_AMP | split: '<meta charset="utf-8">' | join: " " -%}
          {{- article_content_AMP_final -}}

          <p class="article--published-by">
            - published by <span>{{ shop.name }}</span>
          </p>
        </div>

        <div class="read_on_site">
          <span> Read on <a href="{{ article.url }}">DankStop.com</a></span>
        </div>
        
        {%- if settings.social_sharing_blog -%}
        <hr class="hr--clear">
        <h3>{{ 'blogs.article.share_this_post' | t }}</h3>
        {%- include 'social-sharing' -%}
        <hr>
        {%- endif -%}
        
        <div>
          {%- comment -%}
            {%- if blog.comments_enabled? -%}
              <a href="#comments" class="article--comments-link">
                <svg class="comment-bubble" width="29" height="29" viewBox="0 0 29 29"><path d="M21.27 20.058c1.89-1.826 2.754-4.17 2.754-6.674C24.024 8.21 19.67 4 14.1 4 8.53 4 4 8.21 4 13.384c0 5.175 4.53 9.385 10.1 9.385 1.007 0 2-.14 2.95-.41.285.25.592.49.918.7 1.306.87 2.716 1.31 4.19 1.31.276-.01.494-.14.6-.36a.625.625 0 0 0-.052-.65c-.61-.84-1.042-1.71-1.282-2.58a5.417 5.417 0 0 1-.154-.75zm-3.85 1.324l-.083-.28-.388.12a9.72 9.72 0 0 1-2.85.424c-4.96 0-8.99-3.706-8.99-8.262 0-4.556 4.03-8.263 8.99-8.263 4.95 0 8.77 3.71 8.77 8.27 0 2.25-.75 4.35-2.5 5.92l-.24.21v.32c0 .07 0 .19.02.37.03.29.1.6.19.92.19.7.49 1.4.89 2.08-.93-.14-1.83-.49-2.67-1.06-.34-.22-.88-.48-1.16-.74z"></path></svg>
                {{ 'blogs.comments.comments_with_count' | t: count: number_of_comments }}
              </a>
            {%- endif -%}
          {%- endcomment -%}
          {%- include 'tags-article' -%}
        </div>
        
        {%- if blog.next_article or blog.previous_article -%}
        <hr>
        <p class="clearfix">
          {%- if blog.previous_article -%}
          <span class="left">
            {{ 'blogs.article.older_post' | t | prepend: '← ' | link_to: blog.previous_article }}
          </span>
          {%- endif -%}
          {%- if blog.next_article -%}
          <span class="right">
            {{ 'blogs.article.newer_post' | t | append: ' →' | link_to: blog.next_article }}
          </span>
          {%- endif -%}
        </p>
        {%- endif -%}
      </div>
      
      
      {%- comment -%}
      <aside class="sidebar grid-item large--one-fifth grid-border--left">
        {%- include 'blog-sidebar' -%}
      </aside>  
      {%- endcomment -%}

      
    </div>
    
  </body>
</html>