    {%- comment -%}
    <!-- Product meta ================================================== -->
    {%- endcomment -%}
    {%- comment -%}
    <!-- Facebook Open Graph -->
    {%- endcomment -%}
    <meta property="og:type" content="article">
    <meta property="og:title" content="{{ article.title | strip_html | escape }}">
    <meta property="og:url" content="{{ canonical_url }}">
    {%- if article.image -%}
    <meta property="og:image" content="http:{{ article | img_url: 'grande' }}">
    <meta property="og:image:secure_url" content="https:{{ article | img_url: 'grande' }}">
    {%- endif -%}
    {%- if page_description -%}
    <meta property="og:description" content="{{ page_description | escape }}">
    {%- endif -%}
    <meta property="og:site_name" content="{{ shop.name }}">
    {%- comment -%}
    <!-- Twitter user name of the site, based on theme settings -->
    {%- endcomment -%}
    <meta name="twitter:card" content="summary">
    {%- unless settings.social_twitter_link == blank -%}
    <meta name="twitter:site" content="@{{ settings.social_twitter_link | split: 'twitter.com/' | last }}">
    {%- endunless -%}
    <meta name="twitter:title" content="{{ article.title | strip_html | escape }}">
    <meta name="twitter:description" content="{{ article.excerpt_or_content | strip_html | truncatewords: 140, '' | escape }}">
    {%- if article.image -%}
    <meta property="twitter:image" content="https:{{ article | img_url: 'medium' }}">
    {%- endif -%}
  </head>
  <body>
    
    <div hidden class="hidden">{{ content_for_layout }}</div>

    <amp-analytics type="googleanalytics" id="google_analytics_01">
    <script type="application/json">
      {
        "vars": {
          "account": "UA-49184307-1"
