	{%- comment -%}
	<!-- Canonical URL ===================================== -->
	{%- endcomment -%}
	{%- assign new_canonical_url = canonical_url -%}
	{%- assign collections_path = "/collections/" -%}

	{%- if request.path == '/collections/vendors' -%}
		{%- capture query_string -%}{%- include 'query_string_extractor' -%}{%- endcapture -%}
		{%- assign query_string_parts = query_string | split: ',' -%}
		{%- for query_string_part in query_string_parts -%}
			{%- assign query_parts = query_string_part | split: '=' -%}
			{%- assign query_key = query_parts[0] -%}
			{%- assign query_value = query_parts[1] -%}
			{%- if query_key == 'q' -%}
				{%- assign vendor_handle = query_parts[1] | url_decode | handle -%}
				{%- break -%}
			{%- endif -%}
		{%- endfor -%}
		{%- assign new_canonical_url = shop.url | append: collections_path | append: vendor_handle -%}
  
	{% elsif template contains 'list-collections' %}
		{%- assign new_canonical_url = shop.url -%}
  
	{% elsif current_tags.first == 'material_ceramic' and collection.handle == 'bongs' %}
		{%- assign new_canonical_url = shop.url | append: "/collections/ceramic-bongs" -%}
  
	{% elsif template contains 'collection' and collection %}
		{% if collection.handle == 'new-arrivals-klaviyo-collection' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "newest-products" -%}
  
		{% elsif collection.handle == 'all-but-puffco-and-volcano' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "puffco" -%}
  
		{% elsif collection.handle == 'all-but-knockbox' %}
			{%- comment -%}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: 'storz-bickel' -%}
			{%- endcomment -%}
			{%- assign new_canonical_url = shop.url | append: collection.url -%}
  
		{% elsif collection.handle == 'on-sale' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "clearance" -%}
  
		{% elsif collection.handle == 'apparel-lifestyle' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "clothing-accessories" -%}
  
		{% elsif collection.handle == 'dab-rigs-100-and-up' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "dab-rigs-and-oil-rigs" -%}
  
		{% elsif collection.handle == 'all-vapes-except-puffco-davinci-storz' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "davinci" -%}
  
		{% elsif collection.handle == 'sale' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "clearance" -%}
  
		{% elsif collection.handle == 'sale' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "clearance" -%}
  
		{% elsif collection.handle == 'sale' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "clearance" -%}
  
		{% elsif collection.handle == 'sale' %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: "clearance" -%}
  
		{% else %}
			{%- assign new_canonical_url = shop.url | append: collections_path | append: collection.handle -%}
		{% endif %}
    
	{% elsif template contains 'product' %}
		{%- assign new_canonical_url = shop.url | append: product.url -%}
  
	{% endif %}
  
	<!-- Canonical: {{canonical_url}} - New: {{new_canonical_url}} - Collection handle: {{collection.handle}} ========================== -->
	<!-- First tag: {{current_tags.first}} - Current tags: {{current_tags}} ========================== -->
	<link rel="canonical" href="{{ new_canonical_url }}">
  


