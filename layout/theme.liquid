<script>
  document.addEventListener('DOMContentLoaded', function () { 

    document.querySelectorAll('.list-product-tabs a[data-product-tabtop]').forEach(anchor => {
      anchor.addEventListener('click', function() {
        setTimeout(() => {
          document.querySelectorAll('.grid-item').forEach(item => {
            if (!item.querySelector('.product-title')) {
              item.remove();
            }
          });
        }, 1000);
      });
    });

 

    {% if customer.tags contains 'elite_member' %}
    let targetVariantId = 46302742249691;  
    function checkAndRemoveProductFromCart(variantId) { 
        fetch('/cart.js', {
            method: 'GET',
            headers: {
                'Content-Type': 'application/json'
            }
        })
        .then(response => {
            if (!response.ok) {
                throw new Error('Failed to fetch cart');
            }
            return response.json();
        })
        .then(cart => { 
            var itemInCart = cart.items.find(item => item.variant_id === variantId);

            if (itemInCart) { 
                fetch('/cart/change.js', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        id: itemInCart.key,
                        quantity: 0
                    })
                })
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Failed to remove product');
                    }
                    return response.json();
                })
                .then(updatedCart => {
                    console.log('Product removed from cart:', updatedCart);
                })
                .catch(error => {
                    console.error('Error removing product:', error);
                });
            } else {
                console.log('Product not found in the cart.');
            }
        })
        .catch(error => {
            console.error('Error fetching cart:', error);
        });
    } 
    checkAndRemoveProductFromCart(targetVariantId);
    {% endif %}

  })

  
</script>

