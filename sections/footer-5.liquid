                            {{ idiot_named_address_code | split: '[lang2]' | last }}
                        </div>
                        {%- else -%}
                        <div>
                            {{ idiot_named_address_code | split: '[lang2]' | first }}
                        </div>
                        {%- endif -%}
                    </address>
                </div>
                {%- endif -%}

