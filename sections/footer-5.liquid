                                {%- if block.settings.info_icon_3 != blank -%}
                                <span class="info-icon">
                                    {{ block.settings.info_icon_3 }}
                                </span>
                                {%- endif -%}

                                <div class="info-text">
                                    {%- include 'multilang' with block.settings.info_text_3 -%}
                                </div>
                            </li>
