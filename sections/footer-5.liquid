                        {%- if section.settings[display_current_method] -%}
                        {%- assign current_method_icon_name = method | prepend: 'icon_' -%}
                        {%- assign current_method_icon = section.settings[current_method_icon_name] -%}
                      
                        {%- assign w_h_param = ' width="39" height="24" ' -%}
                        {%- if method == 'bitcoin' -%}
                          {%- assign w_h_param = ' width="63" height="24" ' -%}
                        {%- endif -%}
                        
                        <li>
                            <img data-src="{{ current_method_icon | img_url: 'x32', format: 'jpg' }}" {{ w_h_param }} 
                                  src="data:image/svg+xml,%3Csvg%20xmlns='http://www.w3.org/2000/svg'%20viewBox='0%200%2038%2024'%3E%3C/svg%3E" class="lazyload" alt="{{ method }}" loading="lazy">
                        </li>
                        {%- endif -%}
                        {%- endfor -%}
                    </ul>
                </div>
                {%- endif -%}
              
              {%- comment -%} 
