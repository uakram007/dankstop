                        <img width="591" height="126" class="lazyload" src="data:image/svg+xml,%3Csvg%20xmlns='http://www.w3.org/2000/svg'%20viewBox='0%200%20131%2028'%3E%3C/svg%3E" 
                             data-src="{{ section.settings.logo | img_url: 'master' }}" data-sizes="auto" 
                             alt="{{ section.settings.logo.alt | default: shop.name }}" 
                             title="{{ section.settings.logo.alt | default: shop.name }}">
                      </a>
                    </h3>
                    {%- elsif block.settings.footer_logo != blank -%}
                    <h3 class="foot-title">
                        <a href="/" class="logo-ft">
                          <img width="591" height="126" class="lazyload" src="data:image/svg+xml,%3Csvg%20xmlns='http://www.w3.org/2000/svg'%20viewBox='0%200%20131%2028'%3E%3C/svg%3E" 
                               data-src="{{ block.settings.footer_logo | img_url: 'master' }}" data-sizes="auto" 
                               alt="{{ block.settings.footer_logo | default: shop.name }}" 
                               title="{{ block.settings.footer_logo | default: shop.name }}">
                        </a>
                    </h3>
                    {%- endif -%}

                    <div class="location_info">
                        <ul>
                            {%- if block.settings.info_text_1 != blank -%}
                            <li class="text">
                                {%- if block.settings.info_icon_1 != blank -%}
                                <span class="info-icon">
                                    {{ block.settings.info_icon_1 }}
                                </span>
                                {%- endif -%}

                                <div class="info-text">
                                    {%- include 'multilang' with block.settings.info_text_1 -%}
                                </div>
                            </li>
                            {%- endif -%}
                            
                            {%- if block.settings.info_text_2 != blank -%}
