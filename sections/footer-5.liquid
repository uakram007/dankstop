                                {%- if block.settings.info_icon_2 != blank -%}
                                <span class="info-icon">
                                    {{ block.settings.info_icon_2 }}
                                </span>
                                {%- endif -%}

                                <div class="info-text">
                                    {%- include 'multilang' with block.settings.info_text_2 -%}
                                </div>
                            </li>
                            {%- endif -%}

