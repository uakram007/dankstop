 */
hsjs.externalLinksTargetBlank = function(){
  $("a[target!='_blank'], a[target='_blank'][rel!='noopener']")
  .filter(function(index){
    return (this.hostname) && (this.hostname != location.hostname) 
  })
  .attr("target", "_blank")
  .attr("rel", "noopener")
  .addClass("external-link");
}



