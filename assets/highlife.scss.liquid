/*============================================================================
  #Basic Helper Classes
==============================================================================*/
/* minimum font size 16px to prevent auto-zoom on mobile */
.form-control,
input, 
textarea {
  font-size: 16px;
}


.hidden-input,
.hidden, 
[hidden] {
  display: none !important;
}

.visually-hidden:not(:focus):not(:active) {
  clip: rect(0 0 0 0);
  clip: rect(0, 0, 0, 0);
  overflow: hidden;
  position: absolute;
  height: 1px;
  width: 1px;
  margin: -1px; 
  padding: 0; 
  border: 0; 
}

/* fade image in after load */
.lazyload:not(.no-fade),
.lazyloading:not(.no-fade) {
  opacity: 0;
}
.lazyloaded:not(.no-fade) {
  opacity: 1;
  @include transition(opacity 300ms ease);
}
/* .no-js img.lazyload {
  display: none;
}
 */

/* Fix for iFrame, mainly from YotuTube, being wider than container */
iframe {
  max-width: 100%;
}

/* Add pointer to all smooth scroll anchors */
.smooth-scroll-btn {
  cursor: pointer;
}

/* Horizontal scroll crousel */
.scroll-carousel {
  -webkit-flex-wrap: nowrap !important;
  -ms-flex-wrap: nowrap !important;
  flex-wrap: nowrap !important;
  overflow-x: scroll;
  overflow-y: hidden;
  -webkit-overflow-scrolling: touch;
}

.text-bold {
  font-weight: bold;
}


.product-item.grid-item { 
  .imod.ar--1x1 {
    padding-top: 100%;
  }
  .imod.ar--3x4 {
    padding-top: 125%;
  }

  .imod img {
    position: absolute;
    top: 50%;
    left: 0;
    @include prefix(transform, translateY(-50%));
  }
}



