/*============================================================================
  #Tooltips and Help Tips
==============================================================================*/
.help-tip {
  display: inline-block;
  text-align: center;
  background-color: #fff;
  border-radius: 50%;
  width: 18px;
  height: 18px;
  font-size: 12px;
  line-height: 16px;
  font-weight: bold;
  border: 2px solid gray;
  cursor: default;
  z-index: 10;
  &.tooltip-help {
    position: static;
    margin-top: -3px;
    margin-right: 6px;
    vertical-align: middle;
    float: right;
  }
  &:before {
    content: " ";
    display: none;
    height: 0;
    width: 0;
    right: 0;
    top: 0;
    position: absolute;
    pointer-events: none;
  }

}
.tooltip-help {
  pointer-events: auto;
  z-index: $z_index_1 !important;
  position: relative;
  
  &:before,
  &:after,
  .tt-content {
    display: none;
  }
  .tt-content {
    content: attr(data-title);
    width: 257px;
    padding: 10px 15px;
    position: absolute;
    bottom: 100%;
    left: 50%;
    margin-left: -120px;
    margin-top: 10px;
    z-index: $z_index_3;
/*     background: $colorLightestGray; */
    background: #fff;
    border: 1px solid $colorLightGray;
    white-space: normal;
    cursor: default;
    @include prefix(border-radius, 4px);
    @include prefix(box-shadow, 0px 1px 3px rgba(0,0,0,0.37));
  }
  &:after {
    content: " ";
    height: 10px;
    width: 10px;
    position: absolute;
    top: 100%;
    left: 50%;
    z-index: $z_index_2;
    background: $colorLightestGray;
    pointer-events: none;
    border: 1px solid $colorLightGray;
    border-top-width: 0;
    border-left-width: 0;
    @include prefix(box-shadow, 2px 2px 2px -1px rgba(0,0,0,0.37));
  }
  &:not(.html-content) {
    &:before {
      content: attr(data-title);
    }
  }
  &.fading-tip {
    &:before,
    &:after,
    .tt-content  {
      @include prefix(animation, fadeIn 357ms);
    }
  }
  
  &:hover {
    &:before, 
    &:after,
    .tt-content {
      display: block;
    }
    .help-tip:before {
      display: block;
      height: 200%;
      width: 260px;
      right: -38px;
      top: -140%;
      pointer-events: auto;
    }
  }

  &.top { 
    &:before,
    .tt-content {
      bottom: 90%;
      top: auto;
      margin-bottom: 10px;
      @include at-query($min, $postSmall) {
        bottom: 99%;
      }
    }
    &:after {
      bottom: 140%;
      top: auto;
      transform: rotate(45deg);
      @include at-query($min, $postSmall) {
        bottom: 140%;
      }
    }
  }
  &.bottom { 
    &:before,
    .tt-content {
      bottom: 100%;
      top: auto;
      margin-bottom: 10px;
      @include at-query($max, $small) {
        bottom: 90%;
      }
    }
    &:after {
      bottom: 111%;
      top: auto;
      transform: rotate(45deg);
    }
  }
  &.left { 
    &:before,
    .tt-content {
      left: -515%;
      @include at-query($max, $small) {
        left: -450%;
      }
    }
    &:after {
      left: 22.5%;
    }
  }
}



/*============================================================================
  #Pretty Inputs - checkbox, radio, toggle switch, select dropdown
==============================================================================*/
input {
  border: 1px solid #cbcbcb;
  @include prefix(transition, all 357ms ease-in-out);
}
input:focus {
  border: 1px solid #7b7b7b;
}

@-webkit-keyframes ds_pulse {
  0% {
    box-shadow: 0 0 0 0 #bdc3c7
  }
  100% {
    box-shadow: 0 0 0 1.5em rgba(189,195,199,0)
  }
}
@keyframes ds_pulse {
  0% {
    box-shadow: 0 0 0 0 #bdc3c7
  }
  100% {
    box-shadow: 0 0 0 1.5em rgba(189,195,199,0)
  }
}

.pretty {
  position: relative;
  display: inline-block;
  margin-right: 1em;
  white-space: nowrap;
  line-height: 1;

  input {
    position: absolute;
    left: 0;
    top: 0;
    min-width: 1em;
    width: 100%;
    height: 100%;
    z-index: 2;
    opacity: 0;
    margin: 0;
    padding: 0;
    cursor: pointer;
  }
  .state {
    .state-label {
      position: initial;
      display: inline-block;
      font-weight: 400;
      margin: 0;
      text-indent: 1.5em;
      min-width: calc(1em + 2px);
    }
    .state-label:after, 
    .state-label:before {
      content: '';
      width: calc(1em + 2px);
      height: calc(1em + 2px);
      display: block;
      box-sizing: border-box;
      border-radius: 0;
      border: 1px solid #b3b3b3;
      z-index: 0;
      position: absolute;
      left: 0;
      top: calc((0% - (100% - 1em)) - 8%);
      background-color: #fff;
    }
    .state-label:before {
      border-color: #bdc3c7;
    }
  }
  &.p-curve .state .state-label {
    &:after,
    &:before {
      border-radius: 20%;
    }
  }
  &.p-icon .state {
    .icon {
      position: absolute;
      font-size: 1em;
      width: calc(1em + 2px);
      height: calc(1em + 2px);
      left: 0;
      z-index: 1;
      text-align: center;
      line-height: normal;
      top: calc((0% - (100% - 1em)) - 8%);
      border: 1px solid transparent;
      opacity: 0;
      &:before {
        margin: 0;
        width: 100%;
        height: 100%;
        text-align: center;
        display: -webkit-box;
        display: -ms-flexbox;
        display: flex;
        -webkit-box-flex: 1;
        -ms-flex: 1;
        flex: 1;
        -webkit-box-pack: center;
        -ms-flex-pack: center;
        justify-content: center;
        -webkit-box-align: center;
        -ms-flex-align: center;
        align-items: center;
        line-height: 1;
      }
    }
  }
  &.p-svg .state {
    .svg {
      position: absolute;
      font-size: 1em;
      width: calc(1em + 2px);
      height: calc(1em + 2px);
      padding: 2px;
      left: 0;
      z-index: 1;
      text-align: center;
      line-height: normal;
      top: calc((0% - (100% - 1em)) - 8%);
      border: 1px solid transparent;
      opacity: 0;
    }
    svg {
      margin: 0;
      width: 100%;
      height: 100%;
      text-align: center;
      display: -webkit-box;
      display: -ms-flexbox;
      display: flex;
      -webkit-box-flex: 1;
      -ms-flex: 1;
      flex: 1;
      -webkit-box-pack: center;
      -ms-flex-pack: center;
      justify-content: center;
      -webkit-box-align: center;
      -ms-flex-align: center;
      align-items: center;
      line-height: 1;
    }
  }
  &.p-svg input:checked~.state .svg {
    opacity: 1;
  }
  &.p-smooth {
    .icon,
    .svg,
    .state-label:after,
    .state-label:before,
    input:checked+.state .state-label:after {
      transition: all 137ms ease;
    }
  }
  &.p-pulse:not(.p-switch) .state .state-label { 
    &:before {
      border-radius: 50%;
    }
  }
  &.p-pulse:not(.p-switch) input:checked~.state .state-label { 
    &:before {
      -webkit-animation: ds_pulse 353ms;
      animation: ds_pulse 353ms;
    }
  }
  input:checked~.state.p-success .state-label:after, 
  &.p-toggle .state.p-success .state-label:after {
    background-color: #5cb85c!important;
  }
  input:checked~.state.p-success .icon, 
  input:checked~.state.p-success .svg, 
  &.p-toggle .state.p-success .icon, 
  &.p-toggle .state.p-success .svg {
    color: #fff;
    stroke: #fff;
  }
  &.p-icon input:checked~.state .icon {
    opacity: 1;
  }
}





