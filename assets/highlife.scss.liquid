/*============================================================================
  # Dark Mode
==============================================================================*/
$bright_text_color: #fff;
$dim_text_color: rgba(242, 244, 246, 0.7);
$dark_bg_1: #0f171e;
$dark_bg_2: #1b2530;
$dark_bg_3: #232f3e;

/* Light mode */
/* @media (prefers-color-scheme: light) {
}
 */

/* Dark mode */
/* @media (prefers-color-scheme: dark) {
}
 */

body.dark-mode {
  background-color: $dark_bg_1;
  color: $dim_text_color;
  
  /* Site wide elements and classes */
  .h1, .h2, .h3, .h4, .h5, .h6, h1, h2, h3, h4, h5, h6 {
    color: $bright_text_color;
  }
  label {
    color: $dim_text_color;
  }
  table td, table th {
    border-color: $dim_text_color;
  }
  .item-swatch li label,
  img {
/*     filter: invert(1) brightness(50%); */
    filter: brightness(50%);
    @include prefix(transition, filter 1000ms ease-in-out);
  }
  .item-swatch li label:hover, 
  img:hover {
    filter: brightness(100%);
  }
  blockquote {
    border: none;
    background-color: $dark_bg_3;
    color: $dim_text_color;
  }
  ::placeholder,
  :-ms-input-placeholder, 
  ::-ms-input-placeholder {
    color: $dim_text_color;
  }

  .th-box {
    background-color: $dark_bg_2;
  }

  form.search-form {
    background-color: #0f171e;
    border-color: #1b2530;
    background-color: #1b2530;
    border-color: #0f171e;
  }

  .site-footer .logo-ft img,
  .logo-fixed .logo-img img, 
  .header-mb .logo-img img {
    filter: invert(1);
  }
  .modal-backdrop {
    background-color: {{ settings.bg_quick_view | color_modify: 'alpha', 0.8  }};
  }
  .loading-spinner {
    background-color: $dark_bg_3;
    border-radius: 50%;
  }
  .loading-spinner div {
    border-color: #6f6f77 transparent transparent transparent;
  }
  .product-vendor a {
    color: $dim_text_color;
  }
  .item-reviews .ir--count {
    color: $dim_text_color;
    text-decoration-color: $dark_bg_3;
  }
  .item-reviews .ir--stars .star--part {
    color: $dark_bg_3;
  }
  .qty-group {
    background-color: $dark_bg_2;
    color: $dim_text_color;
  }
  .qty-group .button.plus, 
  .qty-group .button.minus,
