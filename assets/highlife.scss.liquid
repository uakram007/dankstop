.ss--state + .ss--btn {
  padding: 0;
}
.ss--state + .ss--btn:before, 
.ss--state + .ss--btn:after {
  content: none;
  display: none;
}
.ss--state:not(:checked) ~ .ss--inner {
  margin: 0;
  overflow: hidden;
  padding-top: 5px;
  padding-bottom: 16px;
  position: absolute;
  top: 100%;
  left: 0;
  width: 100%;
