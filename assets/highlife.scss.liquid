/*============================================================================
  #Shpping Insurance
==============================================================================*/
.route-warning {
  font-size: 10px;
  line-height: 1.4;
  margin: -5px 0 5px;
  margin-bottom: 5px !important;
}
.shipins-wrapper + .cart-des {
  display: none;
}
.shipins-selectors {
  display: inline;
  width: auto;
  margin: 0;
  padding: 0;
  min-height: auto;
  line-height: inherit;
  border: none;
}
.shipins-wrapper {
  margin-bottom: 40px;
  margin-top: 20px;
  text-align: center;
}
#shinsForm {
  @include flexbox();
  @include flex-direction(row);
  @include align-items(center);
  @include justify-content(center);
  background-color: #e2ffe1;
  padding: 20px;
  border-radius: 13px;
  @include prefix(box-shadow, 0px 0px 10px -2px rgba(0,0,0,0.1));
  
  > * {
    margin: 0 20px;
    @include flexbox();
    @include flex-direction(column);
    @include justify-content(center);
  }

  @include at-query ($max, $medium) {
    @include flex-direction(column);
    > * {
      margin: 10px 0px;
    }
  }

  @include at-query (null, $medium, $desktop) {
    @include flex-direction(row);
    @include flex-wrap(wrap);
    > * {
      @include flex(1 0 25%);
      @include align-items(center);
      margin-bottom: 10px;
    }
  }
    
  #shipInsSvg {
    width: 80px;
    height: 80px;
    @include at-query ($max, $large) {
      width: 40px;
      height: 40px;
    }
  }
  .shipins-text {
    text-align: left;
    .shipins-title {
      font-weight: bold;
      font-size: 15px;
      @include at-query ($max, $large) {
        margin-top: 0px;
        font-size: 13px;
      }
    }
    @include at-query ($max, $small) {
      width: 90%;
      text-align: center;
    }
    ul {
      margin: 0;
/*       @include at-query ($max, $small) {
        width: 70%;
      } */
      li {
        display: list-item;
        list-style-type: disc;
        list-style-position: inside;
        text-transform: lowercase;
        font-size: 13px;
        margin: 0;
        &:empty {
          display: none;
        }
        @include at-query ($max, $small) {
          list-style-type: none;
          &+ li {
            margin-top: 5px;
            position: relative;
            &:before {
              content: "~";
              display: block;
              position: absolute;
              top: -11px;
              left: 50%;
            }
          }
        }
        @include at-query ($max, $large) {
          font-size: 10px;
        }
      }
    }
  }
  .shipins-price {
    font-size: 14px;
    font-weight: bold;
    color: #e65246;
  }
  .shipins-atc-container {
    @include at-query ($max, $large) {
      margin-bottom: 0px;
    }
    .shipins-atc {
    }
  }
}

.cart-list .shipins-line-item .details {
  @include align-items(center);
}



