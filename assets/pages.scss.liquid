==============================================================================*/
.human-sitemap-content {
  section {
    padding: 0;
    margin-bottom: 23px;
  }
  .sms {
    > ul {
      > li {
        line-height: 2;
        margin: 0;
        &:hover, 
        &:focus {
          > label {
            > a {
              color: $colorDSBlue;
            }
          }
        }
        > input:checked + label a {
          color: $colorSalmon;
        }
        > label {
          position: relative;
          &:after {
            content: "+";
            position: absolute;
            right: 0;
          }
        }
        > ul {
          > li {
            display: none;
            font-size: 12px;
            line-height: 1;
            padding-left: 13px;
            position: relative;
            background: #fff;
            margin: 0 auto 15px;
            &:first-of-type {
              margin-top: 7px;
            }
            > a:before {
              content: "-";
              display: inline-block;
              position: absolute;
              left: 0;
            }
          }
        }
      }
    }
    a {
      color: $colorTextBody;
      &:hover, 
      &:focus {
        color: $colorDSBlue;
      }
    }
    ul {
      li {
        ul {
          height: 0;
          margin: 0;
          padding: 0;
          overflow: hidden;
        }
        input:checked ~ ul {
          height: auto;
          > li {
            display: inherit;
          }
        }
      }
    }
  }
}
@media screen and (min-width: 481px) and (max-width: 768px) {
  .human-sitemap-content {
    .sms {
      > ul {
        @include prefix(column-count, 2);
      }
    }
