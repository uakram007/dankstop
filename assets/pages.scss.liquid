      margin: 0 0 0 auto;
      display: block;
      width: 70%;
      p {
        padding: 0;
        &:last-child {
          text-transform: uppercase;
        }
      }
    }
    .sub-section {
      background-color: $color_gallery_approx;
      margin-bottom: 40px;
      height: 170px;
      font-size: 0;
      position: relative;
      > div {
        display: inline-block;
        vertical-align: middle;
        max-height: 100%;
        text-transform: uppercase;
      }
      .section-left {
        width: 30%;
        height: 200px;
        position: relative;
      }
      .section-middle {
        width: 20%;
        font-size: 11px;
        padding: 0 0 0 13px;
        span {
          font-weight: bold;
          display: block;
          margin: 1% 0;
        }
        ul {
          @include prefix (column-fill, auto);
          @include prefix (column-width, 160px);
          @include prefix (column-gap, 0);
          height: 130px;
          margin: 0;
        }
      }
      .section-right {
        width: 40%;
        position: absolute;
        right: 1%;
        bottom: 0;
        img {
          width: 32%;
          margin-top: -60px;
          float: right;
          + img {
            margin-right: -30px;
          }
        }
      }
    }
  }
  .mcs-item {
