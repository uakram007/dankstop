//colors
$color_mine_shaft_approx: #333;
$color_pampas_approx: #f2f1ee;

//fonts
$font_0: futura;
$font_1: cursive;


%extend_2 {
  font-size: 12px;
  columns: 1;
  column-fill: auto;
}




.about-us {
  text-align: center;
  h1 {
    color: $colorDSBlue;
  }
  h2 {
    color: $color_mine_shaft_approx;
    font-weight: 900;
  }
  .section {
    font-size: 12px;
    padding-top: 37px;
    padding-bottom: 37px;
    &.sec-1 {
      h2 {
        width: 60%;
        margin-left: auto;
        margin-right: auto;
        padding: 60px 0 10px;
        text-transform: inherit;
      }
      .section--inner {
        font-size: 12px;
        @include prefix(columns, 2);
        @include prefix(column-width, 200px);
        @include prefix(column-fill, auto);
        @include prefix(column-gap, 50px);
        padding: 0 0 130px 0;
      }
    }
    &.sec-2 {
      padding-top: 0;
      padding-bottom: 0;
      .section--inner {
        > div {
          position: relative;
          display: inline-block;
          vertical-align: middle;
          display: block;
          width: 87%;
          margin: 0 auto;
          text-align: initial;
          h2 {
            position: relative;
            text-transform: inherit;
            padding-bottom: 25px;
          }
          p {
            text-align: left;
            font-size: 12px;
          }
        }
        .about-us--map {
          position: relative;
          display: block;
          float: right;
          width: 73px;
          @include at-query($min, $large) {
            width: 25%;
            top: -179px;
          }
        }
      }
    }
    &.sec-3 .section--inner {
      padding-bottom: 120px;
      h2 {
        text-align: center;
        text-transform: inherit;
        font-family: $font_1;
        font-size: 50px;
        line-height: 50px;
        margin-bottom: 110px;
        span {
          display: block;
          font-size: 17px;
          text-align: center;
          text-transform: lowercase;
          text-align: center;
          text-transform: uppercase;
        }
      }
      > div {
        @include prefix(columns, 3);
        @include prefix(column-width, 100px);
        @include prefix(column-fill, auto);
        @include prefix(column-gap, 100px);
        > div {
          margin-bottom: 50px;
        }
        p {
          text-align: center;
          text-transform: uppercase;
          margin-top: 20px;
        }
      }
    }
    &.sec-4 .section--inner {
      h2 {
        text-align: center;
        text-transform: inherit;
        font-family: $font_1;
        font-size: 50px;
        padding: 110px 0 70px;
      }
      > div {
        @include prefix(columns, 3);
        @include prefix(column-width, 200px);
        @include prefix(column-fill, auto);
        @include prefix(column-gap, 100px);
        > div {
          margin-bottom: 100px;
          > div {
            text-align: center;
            text-transform: uppercase;
            &:first-of-type {
              color: $colorPrimary;
              font-weight: 900;
              font-size: 14px;
              padding-bottom: 3px;
            }
            &:nth-of-type(2) {
              font-size: 10px;
              line-height: 10px;
            }
          }
          > img {
          }
        }
      }
    }
    &.sec-5 .section--inner {
      padding: 130px 0 300px;
      h2 {
        text-align: center;
        width: 20%;
        margin: 0 auto;
        padding: 100px 0 30px;
      }
      > div > div {
        &:first-of-type {
          text-align: center;
          font-size: 17px;
          line-height: 33px;
          width: 80%;
          margin: 0 auto;
        }
        &:nth-of-type(2) {
          text-align: center;
          margin: 40px 0 0;
          font-size: 17px;
        }
      }
    }
    &.sec-6 {
      font-size: 37px;
      .section--inner {
        padding-left: 0;
        padding-right: 0;
      }
    }
  }
}
@include at-query($max, $medium) {
  .about-us .section {
    &.sec-1 .section--inner {
      @extend %extend_2;
    }
    &.sec-2 .section--inner {
      > div {
        p {
          width: 100%;
        }
      }
    }
    &.sec-3 .section--inner > div {
      @extend %extend_2;
    }
    &.sec-4 .section--inner > div {
      @include prefix(columns, 1);
    }
    &.sec-5 .section--inner > div > div:first-of-type {
      width: 100%;
    }
  }
}
@include at-query($min, $large) {
  .about-us {
    .section {
      &.sec-6 {
        .section--inner {
          width: 100%;
          font-size: 31px;
        }
      }
    }
  }
}




