      height: 100% !important;
      .ProductImage {
        display: block;
        height: 100%;
        a {
          border: 0;
          display: block;
          height: auto !important;
          padding: 0;
          width: 80%;
          margin: 0 auto;
          img {
            width: 100%;
            max-width: 100%;
          }
        }
      }
    }
    .ProductDetails {
      display: none;
    }
    .p-price {
      display: none;
    }
    .ProductPriceRating {
      display: none;
    }
  }
  .MagicScroll-horizontal {
    .mcs-button-arrow-prev {
      left: -10px;
    }
    .mcs-button-arrow-next {
      right: -10px;
    }
  }
  .prices-section {
    font-size: 0;
    .section-heading {
      font-size: 13px;
    }
    .section-left {
      display: inline-block;
      vertical-align: middle;
      width: 30%;
      font-size: 13px;
      img {
        width: 100%;
      }
    }
    .section-right {
      display: inline-block;
      vertical-align: middle;
      width: 70%;
      font-size: 13px;
      .group-top {
        margin-bottom: 40px;
      }
      h3 {
        color: $black;
        padding: 0 0 0 20px;
        font-weight: bold;
        font-family: inherit;
      }
      p {
      }
      ul {
        list-style: none;
        margin-left: 60px;
      }
    }
  }
