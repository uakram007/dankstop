      color: $color_tapa_approx;
      position: relative;
      ul {
        display: inline-block;
        list-style: none;
        margin: 2% 0 2% 3%;
      }
      img {
        display: inline-block;
        vertical-align: middle;
        width: 22%;
        position: absolute;
        right: 10%;
        top: -30%;
      }
      li {
        margin: 10px 0;
        .span-left {
          display: inline-block;
          width: 160px;
        }
      }
    }
  }
  .quality-section {
    .section-top {
      margin: 40px 0 60px;
      font-size: 0;
      .section-left {
        font-size: 12px;
        width: 50%;
        display: inline-block;
        vertical-align: middle;
        position: relative;
        h3 {
          padding: 0 0 0 20px;
          margin-bottom: 20px;
          font-weight: bold;
          font-family: inherit;
          line-height: 19px;
          font-size: 15px;
          color: $black;
          width: 100%;
          @include prefix (box-sizing, border-box);
        }
        p:last-child {
          margin: 0;
        }
      }
      .section-right {
        font-size: 15px;
        color: $white;
        width: 50%;
        display: inline-block;
        vertical-align: middle;
        background-color: $color_opal_approx;
        max-height: 192px;
        height: 192px;
        position: relative;
        p {
          display: inline-block;
          width: 60%;
          padding: 0 0 0 30px;
          font-size: 17px;
          margin-top: 70px;
        }
        img {
          display: inline-block;
          position: absolute;
          width: 200px;
          vertical-align: middle;
          margin: -72px 0 0 -50px;
        }
      }
    }
    .section-bottom {
      font-size: 0;
      .quality-step {
        display: inline-block;
        width: 25%;
        vertical-align: top;
