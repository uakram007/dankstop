%extend_1 {
  content: '';
  display: inline-block;
  vertical-align: middle;
  height: 20px;
  width: 20px;
  position: absolute;
  left: -25px;
  background-image: url('{{ 'why-shop-online-headshop--checkmark.png' | file_url }}');
