    display: none;
  }

  .main-content {
    padding: 0;
    &> .grid {
      background-color: lighten($colorBGGray, 5%);
      background-color: #ffffff;
      padding: 30px;

      @include at-query($min, $large) {
        @include screen-width-bg;
        padding-top: 100px;
        padding-bottom: 100px;
      }
    }
  }

  h1 {
    color: #353535;
    font-size: 47px;
  }

  .collection-categories-container {
    margin-left: 0;
    @include center-all-stretch;
    @include prefix(flex-wrap, wrap);
  }

  .collection-category-line {
    @include center-all;
    .grid-uniform {
      @include center-all;
    }
  }

  .collection-category {
    width: 100%;
    background-color: #fff;
    padding: 37px 0;
    border-bottom: 5px solid lighten($colorBGGray, 5%);
    border-bottom: 5px solid #f1f3f5;
    @include center-all;

    @include at-query($min, $large) {
      width: 50%;
      border-right: 5px solid lighten($colorBGGray, 5%);
      border-right: 5px solid #f1f3f5;
    }

    &.collection-category--whole {
      width: 100%;
    }
    &.collection-category--half {
      width: 100%;

      @include at-query($min, $large) {
        width: 50%;
      }
    }
    &:last-of-type {
      border-bottom: 0;
    }

    .collection-category--one-half {
      width: 50%;
    }
    .collection-category--one-quarter {
      width: 25%;
    }
    .collection-category--three-quarters {
      width: 75%;
    }

    .collection-category--left {
      @include center-all;
      .image-module {
        margin: 0 30px;
      }
    }
    .collection-category--right {
      @include center-vertical-align-left-column;
    }
    .collection-category--three-quarters {
      ul {
        @include prefix(column-count, 4);
        @include prefix(column-width, auto);
        @include prefix(column-gap, 10px);
      }
    }
    .collection-category--link {

      .collection-category--link-anchor {
        display: block;
        position: relative;
        color: $colorMediumGray;
        overflow:hidden;
        &:hover {
          color: $colorLinkSecondaryHover;
        }

        &.collection-cat--last-hovered {
          color: $colorDSBlue;
          &:before {
            content: "-";
            position: absolute;
            left: -13px;
            top: 30%;
            @include prefix(transform, translateY(-50%));
            width: 10px;
            height: 10px;
          }
        }
      }
    }

  }

  h2 {
    text-transform: lowercase;
    color: $colorDarkGray;;
  }  

}



