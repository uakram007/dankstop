  }

  .product-display {
    background-color: #fff;

    .carousel:not(.flickity-enabled) {
      max-height: 100%;
      overflow: hidden;
      .product-grid-item {
        position: absolute;
        top: 0;
        left: 0;
      }
    }

    .carousel-wrapper {
      position: absolute;
      top: 0;
      width: 70%;
      left: 50%;
      @include prefix(transform, translateX(-50%));
      overflow: visible;
      @include at-query($min, $large) {
        top: initial;
        bottom: -35px;
      }
    }
    .carousel-cell {
      width: 100%;
    }
    .product-grid-item.product-item {
      padding: 0;
      margin: 0;
    }
    .option-line {
      display: none;
    }
    .product-image {
