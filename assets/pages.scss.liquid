==============================================================================*/
.wholesale-online-headshop, 
#wholesale-online-headshop {
  .breadcrumb {
    display: none;
  }
  input, select {
    padding: 9px 32px 9px 12px;
  }
}
.wholesale-content {
/*   margin-top: -30px;
 */
  .section {
    margin-bottom: 113px;
  }

  .ws-header {
    padding: 37px;
    background-color: $colorPrimary;
    text-align: center;
    border-bottom: 23px solid darken($colorPrimary, 10%);
/*     margin: 0 -15px 30px; */
    margin-top: 0;
    margin-bottom: 30px;

    .grid {
      @include at-query($max, $small) {
        margin-left: 0;
      }
      .grid-item {
        position: relative;
        overflow: hidden;
        @include at-query($max, $small) {
          padding-left: 0;
        }
      }
    }
    h1 {
      color: #fff;
      margin: 0;
      font-size: 37px;
      font-weight: lighter;
    }
    .svg-span {
      display: inline-block;
      position: relative;
      margin-bottom: 17px;
      &:before,
      &:after {
        content: "";
        position: absolute;
        border-top: 1px solid #fff;
        width: 237px;
        height: 1px;
        top: 50%;
        margin-top: -1px;
        margin-left: 8px;
      }
      &:before {
        right: 100%;
        margin-right: 30px;
      }
      &:after {
        left: 100%;
        margin-left: 30px;
      }
      svg {
        width: 70px;
        position: relative;
      }
    }
  }

  .ws-greeting {

    .grid {
      @include at-query($max, $small) {
        margin-left: 0;
      }
      .grid-item {
        position: relative;
        overflow: hidden;
        @include at-query($max, $small) {
          padding-left: 0;
        }
      }
    }

    .ws-greeting-text { 

      h2 {
        color: $colorPrimary;
        margin: 0;
        font-weight: 900;
        letter-spacing: 1px;
        font-size: 2em;
        text-align: center;
        margin-bottom: 37px;
        border-bottom: 1px solid $colorPrimary;
        padding-bottom: 13px;
        overflow: hidden;
      }

      h2 {
        div:first-of-type {
          position: relative;
          display: inline-block;
          &:before,
          &:after {
            content: "";
            position: absolute;
            border-top: 1px solid $colorPrimary;
            width: 173%;
            height: 1px;
            top: 50%;
            margin-top: -1px;
            margin-left: 8px;
          }
          &:before {
            right: 100%;
            margin-right: 70px;
          }
          &:after {
            left: 100%;
            margin-left: 70px;
          }
        }
      }
    }

    .ws-account-check {
      text-align: center;
      font-size: 1.5em;
      line-height: 1.37;
      margin-top: 57px;
    }

  }

  .ws-categories {
    text-align: center; 
    .grid-item {
      margin-bottom: 37px;
    }
    .ws-cat-header {
      h2 {
        margin: 0;
        @include prefix(text-stroke, 1px);
        letter-spacing: 2px;
        font-size: 1.73em;
      }
      span {
        text-transform: uppercase;
      }
    }
    .ws-category {
      a {
        display: block;
        padding: 20px;
        border: 1px solid $colorDevilsGray;
        h3 {
          padding: 20px 0;
          text-transform: uppercase;
          color: $colorPrimary;
        }
        img {
          width: auto;
          height: auto;
        }
      }
    }
  }

  .ws-registration {
    .ws-reg-header {
      background-color: $colorDevilsGray;
      text-align: center;
      border-bottom: 23px solid darken($colorDevilsGray, 10%);
      margin: 0 -15px;
      padding: 30px 0;
      @include at-query($min, $large) {
        padding: 57px;
      }
      h2, p {
        color: #fff;
        width: 70%;
        margin: 0 auto 27px;
        text-align: left;
      }
      h2 {
        font-size: 1.37em;
        line-height: 1;
        text-shadow: 2px 3px 1px #000;
        position: relative;
        @include at-query($min, $large) {
          font-size: 3.37em;
        }
        .svg-span {
          position: absolute;
          width: 10%;
          left: -17%;
        }
      }
      .svg-span {
        svg {
          width: auto;
          height: auto;
        }
      }
    }
  }

}

//data-uris
$redExclaimation: "data:image/png;base64,";
$redThumbDown: "data:image/png;base64,";
$greenThumbUp: "data:image/png;base64,";

#wholesale-form-container {
  max-width: 100%;
  margin: 50px auto 113px;
  padding-top: 27px;
  @include at-query($min, $large) {
    width: 83%;
  }
  form {
    fieldset {
      position: relative;
      @include transition(all 137ms);
      border-radius: 3px;
      border: 1px solid $colorDevilsGray;
      background: #fff;
      padding: 2vw;
      @include at-query($min, $large) {
        padding: 0 7vw calc(4vw - 20px);
      }
      legend {
        background: #fff;
        text-transform: uppercase;
        letter-spacing: 1px;
        line-height: 33px;
        padding: 0 18px;
        position: relative;
        top: -8px;
        margin: 0;
        margin-bottom: 53px;
        font-size: 2.3em;
        @include at-query($min, $large) {
          font-size: 3em;
        }
      }
    }
    input,
    select,
    textarea {
      @include transition(all 137ms);
      border-radius: 1px;
      border: 1px solid $colorDevilsGray;
      background: $colorLightestGray;
      box-shadow: inset 0 1px 1px rgba(0,0,0,0.075);
      padding: 0 1em !important;
      line-height: 40px;
      margin: 0;
      &:required {
        &:invalid {
          background-image: url($redExclaimation);
          background-position: right top;
          background-repeat: no-repeat;
        }
        &:valid {
          background-image: url($greenThumbUp);
          background-position: right top;
          background-repeat: no-repeat;
        }
      }
      &:focus:invalid {
        background-image: url($redThumbDown);
        background-position: right top;
        background-repeat: no-repeat;
      }
      &:disabled {
        color: #999;
      }
    }
    textarea {
      line-height: 1.37;
      padding: .7em 1em !important;
    }
    input::-webkit-input-placeholder {
      color: #666;
    }
    input:-moz-placeholder {
      color: #666;
    }
    input::-moz-placeholder {
      color: #666;
    }
    input:-ms-input-placeholder {
      color: #666;
    }
    textarea::-webkit-input-placeholder {
      color: #666;
    }
    textarea:-moz-placeholder {
      color: #666;
    }
    textarea::-moz-placeholder {
      color: #666;
    }
    textarea:-ms-input-placeholder {
      color: #666;
    }
    .form-row {
      display: flex;
      flex-direction: row;
      margin: 0 auto 9px;
      position: relative;
      @include at-query($max, $small) {
        flex-direction: column;
      }
      &.source-row {
        margin-bottom: 27px;
      }
      &.subject-row,
      &.password-row,
      &.store-address-row {
        margin-bottom: 53px;
      }
      .form-left {
        position: relative;
        @include center-vertical;
        flex: 3;
        @include at-query($max, $small) {
          margin: 7px 0 7px 23px;
        }
        &.required-field:before {
          content: "*";
          position: absolute;
          top: 32%;
          font-size: 30px;
          color: darken($colorPaleBurgundy, 20%);
        }
        label {
          height: 42px;
          margin: 0;
          text-transform: uppercase;
          @include center-vertical;
          i {
            @include transition(all 137ms);
            font-size: 27px;
            margin-right: 13px;
            min-width: 37px;
            text-align: center;
          }
          span {
            @include transition(all 137ms);
            font-size: 15px;
            font-weight: bold;
            line-height: 1.27;
          }
        }
      }
      &.store-address-row .form-left,
      &.notes-row .form-left,
      &.captcha-row .form-left {
        display: block;
      }
      .form-right {
        flex: 5;
        @include at-query($max, $small) {
          margin-left: 73px;
        }
        .input-line {
          display: flex;
          flex-direction: row;
          .line-left {
            margin: 0 8px 0 0;
            flex: 1;
            &.city-line {
              flex: 2;
            }
          }
          .line-right {
            position: relative;
            flex: 1;
            &.country-line {
              flex: 2;
            }
          }
          + .input-line {
            margin-top: 9px;
          }
        }
      }
      &:not(.disabled-field):not(.response) {
        &:hover {
          i,
          span {
            color: lighten($colorPrimary, 15%);
          }
          input,
          select,
          textarea {
            border-color: lighten($colorPrimary, 15%);
            &.gray-border {
              border-color: lighten($colorPrimary, 45%);
            }
          }
        }
        input,
        select,
        textarea {
          &:focus {
            border-color: lighten($colorPrimary, 15%);
            box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(106, 218, 120,0.6);
          }
          &:not([disabled]):invalid:focus {
            border-color: #f47d77 !important;
            border-color: #f9bdba !important;
            box-shadow: inset 0 1px 1px rgba(0,0,0,0.075),0 0 8px rgba(249, 189, 186,0.6);
          }
        }
        select:focus {
          &:before {
            background-color: #666;
          }
        }
      }
      &:hover select {
        &:before {
          background: rgb(106, 218, 120);
        }
        &.hover span.gray-border {
          border-color: rgb(106, 218, 120);
        }
      }
      select:hover:before {
        background-color: #666;
      }
      .country-line,
      &.source-row .form-right {
        &:before {
          content: "\f078";
          font-family: "Font Awesome 5 Free";
          font-weight: 900;
          text-rendering: auto;
          -webkit-font-smoothing: antialiased;
          -moz-osx-font-smoothing: grayscale;
          @include center-all;
          text-align: center;
          position: absolute;
          right: 0;
          border-radius: 0;
          height: calc(100% - 2px);
          max-height: 40px;
          width: 42px;
          margin: 1px;
          background: $colorDevilsGray;
          color: #fff;
          pointer-events: none;
          cursor: pointer;
        }
      }
      &.submission {
        margin: 0 auto;
        position: relative;
        background: #fff;
        width: 83%;
        bottom: -59px;
        @include at-query($min, $large) {
          bottom: -62px;
          width: 73%;
        }
        .btn {
          background-color: #39b54a;
          border-color: #39b54a;
          color: #fff;
          margin: 0 auto;
          width: 85%;
          white-space: normal;
          font-size: 17px;
          padding: 17px 23px;
          @include at-query($min, $large) {
            font-size: 1.5em;
            padding: 17px 33px;
          }
          
          &:hover {
            background-color: #fff;
            border-color: #39b54a;
            color: #39b54a;
          }
        }
      }    
    }
  }



  .form-row {
    &.response {
      min-height: 20px;
      visibility: hidden;
      text-align: center;
      margin-top: 0;
    }
    #form-response {
      position: relative;
      top: 0;
      bottom: 0;
      width: 100%;
      height: 100%;
      text-align: center;
      margin: 0 auto;
      display: none;
      &.submit-success .fa {
        color: $colorPrimary;
      }
      &.submit-error .fa {
        color: $colorPaleBurgundy;
      }
      .fa {
        font-size: 90px;
        font-size: 57px;
        margin-bottom: 20px;
      }
    }
  }

}



