/*============================================================================
  #Articles
==============================================================================*/
.template-article {
  .article--published-by {
    font-size: 10px;
    font-style: italic;
  }
}

.article-body {
  margin-bottom: 30px;
  @include center-all-stretch;
  @include prefix(flex-wrap, wrap);
  
  .article--header {
    margin: 30px 0px;
  }
  .article--content {
    img {
      margin: 0 auto;
      display: block;
      @include at-query($max, $medium) {
        width: calc( 100% + 30px );
        max-width: none;
        margin-left: -15px !important;
        float: none;
      }
    }
  }
  .article--title {
    font-weight: 700;
    font-style: normal;
    font-size: 32px;
    margin-left: -2px;
    margin-bottom: 0;
    line-height: 1.04;
    letter-spacing: -.028em;
    @include at-query($min, $large) {
      font-size: 37px;
      margin-left: -2.5px;
      line-height: 1.04;
      letter-spacing: -.028em;
    }
  }
  .article--sub-title {
    color: rgba(0,0,0,.44);
    font-size: 12px;
    font-family: medium-ui-sans-serif-text-font,-apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,Oxygen,Ubuntu,Cantarell,"Open Sans","Helvetica Neue",sans-serif;
    letter-spacing: 0;
    font-weight: 400;
    font-style: normal;
    @include at-query($min, $large) {
      font-size: 15px;
    }
    .dot-sep {
      &:after {
        content: "\2022";
      }
    }
  }
  p {
    text-rendering: optimizeLegibility;
    font-family: medium-content-serif-font,Georgia,Cambria,"Times New Roman",Times,serif;
    letter-spacing: .01rem;
    font-weight: 400;
    font-style: normal;
    color: rgba(0,0,0,.8);    
    font-size: 18px;
    line-height: 1.58;
    letter-spacing: -.004em;
    @include at-query($min, $large) {
      line-height: 1.58;
      letter-spacing: -.003em;
    }
  }
  hr {
    margin: 15px 0 15px 0;
  }
  .article--share-this-title {
    margin: 0;
  }
  .social-sharing {
    display: inline;
    padding: 0;
    a {
      margin-top: 10px;
      margin-bottom: 0;
    }
  }
  .article--comments-link {
    margin-top: 10px;
    color: rgba(0,0,0,.44);
    float: right;
    svg {
      fill: rgba(0,0,0,.6);
      vertical-align: middle;
    }
    &:hover {
      color: rgba(0,0,0,.6);
      svg {
        fill: rgba(0,0,0,.8);
      }
    }
  }
  .article--tag-list {
    margin: 0;
    li {
      display: inline-block;
      line-height: 22px;
      font-family: medium-ui-sans-serif-text-font,-apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,Oxygen,Ubuntu,Cantarell,"Open Sans","Helvetica Neue",sans-serif;
      font-weight: 400;
      font-style: normal;
      color: rgba(0,0,0,.44);
      font-size: 13px;
      letter-spacing: 0;
      text-decoration: none;
      background: #fafafa;
      border-radius: 3px;
      border: 1px solid #f0f0f0;
      padding: 5px 10px;
      margin-right: 8px;
      margin-bottom: 8px;
      border: none;
      background: rgba(0,0,0,.05);
      a {
        color: rgba(0,0,0,.6);
      }
      &:hover {
        background: rgba(0,0,0,.1);
        a {
          color: rgba(0,0,0,.6);
        }
      }
    }
  }
  
  .sidebar {
    display: none;
  }
  
  .article--floated-shares {
    position: fixed;
    top: 80px;
    left: 80px;
    width: 64px;
    h3 {
      font-weight: 400;
      text-transform: uppercase;
      font-size: 13px;
      text-align: center;
    }
    .social-sharing {
      display: block;
      a {
        display: block;
        margin: 0 0 7px;
        width: auto;
        background-color: transparent;
        color: rgba(0,0,0,.6);
        border: 1px solid rgba(0,0,0,.6);
        border-radius: 4px;
        .svg-icon svg {
          width: 25px;
          fill: rgba(0,0,0,.6);
          @include transition(all 357ms);
       }
        span:nth-of-type(2) {
          font-size: 10px;
          font-weight: 300;
          padding-right: 5px;
          margin-top: -1px;
        }
        .share-count {
          height: 14px;
          line-height: 13px;
          top: 4px;
          border-radius: 2px;
        }
        &:hover {
          color: #fff;
          border-color: transparent;
          &.share-facebook {
            background-color: #3b5998;
          }
          &.share-twitter {
            background-color: #00aced;
          }
          &.share-pinterest {
            background-color: #cb2027;
          }
          &.share-fancy {
            background-color: #4999dc;
          }
          &.share-google {
            background-color: #dd4b39;
          }
          .svg-icon svg {
            fill: #fff;
          }
        }
      }
    }
    .article--comments-link {
      width: 100%;
      text-align: center;
    }
  }
  
  .article--footer {
    background: #fafafa;
    margin-left: -15px;
    margin-right: -15px;
    padding-left: 15px;
    padding-right: 15px;
    margin-bottom: -40px;
    padding-bottom: 30px;
    margin-top: -15px;
    padding-top: 20px;
    @include at-query($min, $large) {
      margin-left: calc( (100vw - 100%) / 2 * -1 );
      margin-right: calc( (100vw - 100%) / 2 * -1 );
      padding-left: calc( (100vw - 100%) / 2 );
      padding-right: calc( (100vw - 100%) / 2 );
    }
    .articles-sec {
      margin-top: 10px;
      background-color: transparent;
    }
    .article-list {
      .article {
        .article--inner {
          background: #fff;
          box-shadow: 0 1px 7px rgba(0,0,0,.05);
          border: 1px solid rgba(0,0,0,.04);
          border-radius: 4px;
          overflow: hidden;
          z-index: 1;
          position: relative;
          width: 100%;
          @include at-query($min, $large) {
            &.grid {
              margin-left: -15px;
              .grid-item.article {
                padding-left: 15px;
              }
            }
          }
        }
        .article--details {
        }
      }
    }
  }
  .comments-container {
  }
  .comment-meta-container {
    margin: 0 0 15px 0;
  }
  .comments-header {
    .separator {
      display: none;
      @include at-query($min, $large) {
        display: initial;
      }
    }
    a {
      display: block;
      @include at-query($min, $large) {
        display: initial;
      }
    }
  }
/*   #comments {
    background: #fafafa;
  } */
  .comment {
    background: #fff;
    box-shadow: 0 1px 4px rgba(0,0,0,.04);
    border: 1px solid rgba(0,0,0,.09);
    border-radius: 3px;
    padding: 15px 20px;
    margin-bottom: 20px;
    p {
      font-size: 18px;
      line-height: 1.58;
      letter-spacing: -.004em;
    }
    p:last-of-type {
      margin: 0;
    }
  }
  .comment-meta-container {
    > * {
      display: inline-block;
      vertical-align: middle;
    }
    .comment-avatar {
      width: 36px;
      height: 36px;
      border-radius: 100%;
      overflow: hidden;
    }
    .comment-meta {
      color: rgba(0,0,0,.44);
      line-height: 1.4;
      padding-left: 10px;
      text-rendering: auto;
      span {
        display: block;
      }
      span:first-of-type {
        font-size: 14px;
        color: lighten($colorPrimary, 13%);
      }
      span:nth-of-type(2) {
        font-size: 12px;
      }
    }
  }
}


