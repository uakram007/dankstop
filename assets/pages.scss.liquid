                min-width: 63px;
                @include border-radius(1px);
                margin-right: 20px;
                text-align: center;
            }
        }
        
        .panel-title {
            font-size: $font_size;
            line-height: $font_size + 12;      
            font-weight: 500;
            margin-bottom: 0;
            
            a {
                @include border-radius(1px);
                padding-right: 65px;
                position: relative;
                
                &:before {          
                    position: relative;
                    top: 3px;
                }
                
                &:after {
                    display: inline-block;
                    font-size: $font_size;
                    font-weight: normal;
                    line-height: $font_size + 12;   
                    text-align: right;
                    content: "show";
                    color: $breadcrumb_color;
                    position:absolute;
                    right: 25px;
                    top: 0;
                    padding: 9px 0 7px;
                    text-transform: capitalize;
                }
                
                &:not(.collapsed) {
                    &:after {
                        content: "hide";
                        color: #fff;
                    }
                }
            }
        }
        
        .panel-body {
            padding-top: 26px;
            padding-bottom: 10px;
            line-height: $font_size + 10;
            
            &:before {
                position: relative;
                top: 3.5px;
            }
            
            p {
                &:not(:last-child),
                &:not(:last-of-type) {
                    margin-bottom: 2rem;
                }
            }
        }
    }
    
    .contact-link {
        @include flexbox();
        @include justify-content(flex-end);
        @include align-items(center);
        
        .text {
            font-size: $font_size;
            line-height: $font_size + 12;
            margin-right: 20px;
        }
        
        .btn {
            @include button_3();
            min-width: 215px;
            
            &:hover {
                @include button_1();
            }
        }
    }
}

/* ----- Quickview Page ------- */
.ajax-quickview {
    .halo-modal-content {
        .halo-modal-body {
            padding: 50px 70px 50px 60px;
            max-height: 85vh;
        }
    }
}

.quickview-tpl {
    .product-photos {
        -webkit-box-flex: 0;
        -ms-flex: 0 0 410px;
        flex: 0 0 410px;
        max-width: 410px;
    }
    
    .product-shop {
        margin-top: -8px;
        -webkit-box-flex: 0;
        -ms-flex: 0 0 calc(100% - 410px);
        flex: 0 0 calc(100% - 410px);
        @include calc(max-width, '100% - 410px');
        
        .product-title {
            a {
                color: $page_title_color;
            }
        }
    }
    
    .product-img-box {
        .slider-nav {
            .item {
                padding-bottom: 5px;
                border-bottom: 2px solid rgba(255, 255, 255, 0);
